<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-studyguide-sgd">
        <comment>StudyGuide Document</comment>
        <glob pattern="*.sgd" weight="80"/>
    </mime-type>
    <mime-type type="application/x-studyguide-sgc">
        <comment>StudyGuide Collection</comment>
        <glob pattern="*.sgc" weight="80"/>
    </mime-type>
    <mime-type type="application/x-studyguide-sga">
        <comment>StudyGuide Autosave</comment>
        <glob pattern="*.sga" weight="80"/>
    </mime-type>
</mime-info>
