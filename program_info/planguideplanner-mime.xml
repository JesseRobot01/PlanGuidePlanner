<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-planguideplanner-pgd">
        <comment>Plan Guide Planner File</comment>
        <glob pattern="*.pgd" weight="80"/>
    </mime-type>
    <mime-type type="application/x-planguideplanner-pgm">
        <comment>Plan Guide Planner Multi File</comment>
        <glob pattern="*.pgm" weight="80"/>
    </mime-type>
    <mime-type type="application/x-planguideplanner-pgx">
        <comment>Plan Guide Planner XMl based File</comment>
        <glob pattern="*.pgx" weight="80"/>
    </mime-type>
</mime-info>
